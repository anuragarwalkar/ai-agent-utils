{
  "name": "ai-agent-chrome-extension",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --mode development",
    "build:prod": "tsc -b && vite build --mode production",
    "build:base": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "package": "npm run build:prod && npm run zip",
    "zip": "cd dist && zip -r ../ai-agent-utils-extension.zip . -x '*.DS_Store'",
    "validate": "npm run lint && npm run build:prod"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "langchain": "^0.3.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/chrome": "^0.0.298",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
